<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>

<?import javafx.scene.control.ListView?>
<fx:root type="javafx.scene.layout.StackPane" xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml" maxHeight="Infinity" HBox.hgrow="ALWAYS"
         style="-fx-background-color:#f5f5f5" stylesheets="@main_view.css">
    <VBox spacing="30" styleClass="main-container">
        <Label fx:id="title" styleClass="page-title"/>
        <ListView fx:id="emailList" VBox.vgrow="ALWAYS" maxWidth="Infinity"/>
    </VBox>


</fx:root>
